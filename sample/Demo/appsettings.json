{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": "true"
    }
  },
  "AllowedHosts": "*",
  "Throttle": {
    "EnableGlobalFilter": false,
    "UseDistributed": false,
    "Policy": {
      "EnableHttpMethod": true,
      "IpWhitelist": [ "10.23.1.0 - 10.23.1.220" ],
      "UserAgentRules": {
        "Chrome": {
          "MaxRate": 2,
          "MaxBurst": 2,
          "PeriodType": "Min"
        }
      }

    }
  }
}
