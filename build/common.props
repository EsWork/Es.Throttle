<Project>
  <Import Project="dependencies.props" />
  <Import Project="version.props" />

  <PropertyGroup>
    <Description>rate limit</Description>
    <Product>Throttle libraries</Product>
     <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com//EsWork/Es.Throttle</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)Es.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>True</SignAssembly>
    <PublicSign Condition="'$(OS)' != 'Windows_NT'">true</PublicSign>
    
    <Copyright>2019 EsWork, Inc.</Copyright>
    <Authors>EsWork;v.la@live.cn</Authors>
    <PackageTags>rate limit;throttle</PackageTags>
    <PackageIconUrl>http://s.gravatar.com/avatar/28dfa5190889f7190e74b683fc30b827?s=80</PackageIconUrl>
    <PackageProjectUrl>https://github.com/EsWork/Es.Throttle</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageLicenseType>MIT</PackageLicenseType>
  </PropertyGroup>

   <ItemGroup>
        <None Include="../../LICENSE" Pack="true" PackagePath="LICENSE"/>
    </ItemGroup>

</Project>
